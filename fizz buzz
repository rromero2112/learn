#include <stdio.h>

int main()
{
  for (int i = 1; i <= 100; i++)   //loop to output numbers 1 to 100
  {
    if(i % 3 == 0)   //if statement checking numbers divisible by 3
    {
      printf("fizz\n");   //output for numbers divisible by 3
    }
    else if (i % 5 == 0)   //else if statement checking numbers divisible by 5
    {
      printf("buzz\n");   //output for numbers divisible by 5
    }
    else   //else statement for every other number not divisible by 3 or 5
    {
    printf("%d\n", i);   // output for all other numbers
    }
  }
  return 0;
}
